  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.7",
    "@types/sap__xsenv": "^2.0.3",
    "typedoc": "^0.22.9",
    "typedoc-plugin-markdown": "^3.11.6",
    "typescript": "^4.4.4"
  }
